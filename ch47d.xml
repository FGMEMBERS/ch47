<!--
Syd Adams
NOTES:
Blades (fiberglass)
-12 degree twist
32 inch chord
weight ?
Frt Rotor hub 9 degree forward pitch
Rr Rotor hub 4 degree forward pitch
-->

<airplane mass="23500">

    <!-- Approach configuration -->
    <approach speed="47" aoa="16">
        <control-setting axis="/controls/engines/engine[0]/throttle" value="0.3"/>
        <control-setting axis="/controls/engines/engine[0]/mixture" value="1.0"/>
        <control-setting axis="/controls/flight/flaps" value="1.0"/>
    </approach>

    <!-- Cruise configuration -->
    <cruise speed="170" alt="0">
        <control-setting axis="/controls/engines/engine[0]/throttle" value="1.0"/>
        <control-setting axis="/controls/engines/engine[0]/mixture" value="1.0"/>
        <control-setting axis="/controls/flight/flaps" value="0.0"/>
    </cruise>


    <rotor
        name="main"
        x="5.6"
        y="0.0"
        z="2.4"
        nx="0"
        ny="0"
        nz="1"
        fx="1"
        fy="0"
        fz="0"
        ccw="0"
        maxcollective="18.0"
        mincollective="1.0"
        mincyclicele="-2.0"
        maxcyclicele="2.0"
        mincyclicail="-10"
        maxcyclicail="10"
        diameter="18.29"
        numblades="3"
        weightperblade="150"
        relbladecenter="0.63"
        dynamic="1.0"
        rpm="225"
        rellenflaphinge="0.03"
        delta3="0"
        delta="0.1"
        pitch-a="10"
        pitch-b="15"
        flapmin="-18"
        flapmax="18"
        flap0="-5"
        flap0factor="0"
        notorque="0"
        dragfactor="0.30"
        translift-ve="20"
        translift-maxfactor="1.3"
        ground-effect-constant="0.1"
        twist="-12.0"
        taper="1"
        chord="0.8128"
        number-of-segments="8"
        number-of-parts="8"
        rel-len-where-incidence-is-measured="0.7"
        rel-len-blade-start="0.250"
        tiltcenterx="5.6"
        tiltcentery="0"
        tiltcenterz="2.4"
        mintiltyaw="0"
        mintiltpitch="-9"
        mintiltroll="0"
        maxtiltyaw="0"
        maxtiltpitch="9"
        maxtiltroll="0"
          airfoil-lift-coefficient="3.85"
          airfoil-drag-coefficient0="0.0075"
          airfoil-drag-coefficient1="0.25"
          incidence-stall-zero-speed="15"
          incidence-stall-half-sonic-speed="14.5"
          lift-factor-stall="0.18"
          stall-change-over="5.5"
          drag-factor-stall="2.0"
          cyclic-factor="0.8"
          rotor-correction-factor="0.85">
    <control-input axis="controls/flight/rudder" control="CYCLICAIL"
                       src0="-1.0" src1="1.0"
                       dst0="-0.8" dst1="0.8"/>
        <control-input axis="controls/flight/aileron" control="CYCLICAIL"
                       src0="-1.0" src1="1.0"
                       dst0="-0.3" dst1="0.3"/>
        <control-input axis="controls/flight/elevator" control="COLLECTIVE"
                       src0="-1.0" src1="1.0"
                       dst0="0.5" dst1="-0.5"/>
        <control-input axis="controls/engines/engine[1]/throttle" control="COLLECTIVE"
                       src0="-0.0" src1="1.0"
                       dst0="1.0" dst1="-1.0"/>
        <control-input axis="controls/rotor/pitch" control="TILTPITCH"
                           src0="0.0" src1="1.0"
                 dst0="0" dst1="-1.0"/>
    </rotor>

    <rotor
        name="main2"
        x="-5.6"
        y="0.0"
        z="3.7"
        nx="0.0"
        ny="0"
        nz="1.0"
        fx="1"
        fy="0"
        fz="0"
        ccw="1"
        maxcollective="18"
        mincollective="1.0"
        mincyclicele="-2.0"
        maxcyclicele="2.0"
        mincyclicail="-10.0"
        maxcyclicail="10.0"
        diameter="18.29"
        numblades="3"
        weightperblade="150"
        relbladecenter="0.63"
        dynamic="1.0"
        rpm="225"
        rellenflaphinge="0.03"
        delta3="0"
        delta="0.1"
        pitch-a="10"
        pitch-b="15"
        flapmin="-18"
        flapmax="18"
        flap0="-5"
        flap0factor="0"
        notorque="0"
        dragfactor="0.30"
        translift-ve="20"
        translift-maxfactor="1.3"
        ground-effect-constant="0.1"
        twist="-12.0"
        taper="1"
        chord="0.8128"
        number-of-segments="8"
        number-of-parts="8"
        rel-len-where-incidence-is-measured="0.7"
        rel-len-blade-start="0.250"
        tiltcenterx="-5.6"
        tiltcentery="0"
        tiltcenterz="3.7"
        mintiltyaw="0"
        mintiltpitch="-4"
        mintiltroll="0"
        maxtiltyaw="0"
        maxtiltpitch="4"
        maxtiltroll="0"
          airfoil-lift-coefficient="3.85"
          airfoil-drag-coefficient0="0.0075"
          airfoil-drag-coefficient1="0.25"
          incidence-stall-zero-speed="15"
          incidence-stall-half-sonic-speed="14.5"
          lift-factor-stall="0.18"
          stall-change-over="5.5"
          drag-factor-stall="2.0"
          cyclic-factor="0.8"
          rotor-correction-factor="0.85">

        <control-input axis="controls/flight/rudder"
            control="CYCLICAIL"
            invert="true"
            src0="-1.0" src1="1.0"
            dst0="-0.8" dst1="0.81"/>
        <control-input axis="controls/flight/aileron"
            control="CYCLICAIL"
            src0="-1.0" src1="1.0"
            dst0="-0.3" dst1="0.3"/>
        <control-input axis="controls/flight/elevator"
            control="COLLECTIVE"
            src0="-1.0" src1="1.0"
            dst0="-0.5" dst1="0.5"/>
        <control-input axis="controls/engines/engine[1]/throttle"
            control="COLLECTIVE"
            src0="-0.0" src1="1.0"
            dst0="1.0" dst1="-1.0"/>
          <control-input axis="controls/rotor/pitch"
            control="TILTPITCH"
                src0="-1.0" src1="1.0"
                 dst0="1.0" dst1="-1.0"/>
    </rotor>

    <rotorgear
        max-power-engine="5000"
        max-power-rotor-brake="100"
        rotorgear-friction="1.4"
        engine-prop-factor="0.03"
        engine-accel-limit="10"
        yasimdragfactor="10"
        yasimliftfactor="140">

        <control-input axis="controls/engines/engine[0]/magnetos"
            control="ROTORGEARENGINEON"/>
        <control-input axis="controls/rotor/brake" control="ROTORBRAKE"
            src0="0.0" src1="1.0"
            dst0="0.0" dst1="1.0"/>
    </rotorgear>

    <cockpit x="6.0" y="0" z="0.5"/>

    <fuselage ax="6.5" ay="0" az="0.0" bx="-6.5" by="0" bz="0.0"
          width="2.6" taper="1.0" idrag="0"/>

    <fuselage ax="6.28" ay="0" az="1.72" bx="3.86" by="0" bz="1.72"
          width="0.9" taper="1.0" idrag="0"/>

    <fuselage ax="-3.66" ay="0" az="2.2" bx="-7.4" by="0" bz="2.2"
          width="1." taper="1.0" idrag="0" cy="2" cx="2"/>

    <weight x="6.0"  y="0.5" z="0" mass-prop="sim/weight[0]/weight-lb" size="180"/>
    <weight x="6.0"  y="-0.5" z="0" mass-prop="sim/weight[1]/weight-lb" size="180"/>
    <weight x="0.0"  y="0" z="-0.5" mass-prop="sim/weight[2]/weight-lb" size="0"/>

    <gear x="1.78" y="1.5" z="-2.0"
        compression="0.5"
        spring="10.5"
        damp="1.0">
        <control-input axis="controls/flight/rudder" control="STEER" square="true"/>
        <control-input axis="controls/gear/brake-left" control="BRAKE"/>
        <control-input axis="controls/gear/brake-parking" control="BRAKE"/>
    </gear>

    <gear x="1.78" y="-1.5" z="-2.0"
        compression="0.5"
        spring="10.5"
        damp="1.0">
        <control-input axis="controls/flight/rudder" control="STEER" square="true"/>
        <control-input axis="controls/gear/brake-right" control="BRAKE"/>
        <control-input axis="controls/gear/brake-parking" control="BRAKE"/>
    </gear>

    <gear x="-4.45" y="1.5" z="-1.7"
        compression="0.5"
        spring="10.5"
        damp="1.0">
        <control-input axis="controls/gear/brake-left" control="BRAKE"/>
        <control-input axis="controls/gear/brake-parking" control="BRAKE"/>
    </gear>

    <gear x="-4.45" y="-1.5" z="-1.7"
        compression="0.5"
        spring="10.5"
        damp="1.0">
        <control-input axis="controls/gear/brake-right" control="BRAKE"/>
        <control-input axis="controls/gear/brake-parking" control="BRAKE"/>
    </gear>

<!--two main tanks 1034 gal total-->

    <tank x="0" y="1.5" z="-0.5" capacity="3464"/>
    <tank x="0" y="-1.5" z="-0.5" capacity="3464"/>
    <tank x="0" y="0" z="-0.5" capacity="0"/>


<ballast x="-5" y="0.0" z="-0.5" mass="1000"/>

</airplane>